  "dependencies": {
    "@anthropic-ai/foundry-sdk": "^0.2.0",
    "chalk": "^5.3.0",
    "cli-highlight": "^2.1.11",
    "commander": "^12.1.0",
    "dotenv": "^17.2.3",
    "fast-glob": "^3.3.2",
    "ink": "^5.0.1",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^14.1.3",
    "marked-terminal": "^7.2.1",
    "openai": "^4.73.0",
    "ora": "^8.1.1",
